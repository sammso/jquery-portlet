<?xml version="1.0"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
	<portlet>
		<portlet-name>jquery-portlet-sample</portlet-name>
		<display-name>jQuery Portlet Script Sample</display-name>
		<portlet-class>jqueryportlet.sample.JspPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/WEB-INF/jsp/sample.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>jQuery Portlet Script Sample</title>
			<short-title>jQPS Sample</short-title>
		</portlet-info>
	</portlet>
	<portlet>
		<portlet-name>jquery-portlet-target</portlet-name>
		<display-name>jQuery Portlet Script Target</display-name>
		<portlet-class>jqueryportlet.sample.JspPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/WEB-INF/jsp/target.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>jQuery Portlet Script Sample Target</title>
			<short-title>jQPS Target</short-title>
		</portlet-info>
	</portlet>
	<portlet>
		<portlet-name>jquery-portlet-source</portlet-name>
		<display-name>jQuery Portlet Script Source</display-name>
		<portlet-class>jqueryportlet.sample.JspPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/WEB-INF/jsp/source.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>jQuery Portlet Script Sample Source</title>
			<short-title>jQPSS Source</short-title>
		</portlet-info>
	</portlet>
</portlet-app>